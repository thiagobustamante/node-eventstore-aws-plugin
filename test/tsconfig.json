{
    "compilerOptions": {
        "declaration": true,
        "lib": [
            "es6",
            "dom"
        ],
        "module": "commonjs",
        "newLine": "LF",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": false,
        "noUnusedParameters": false,
        "noUnusedLocals": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strictNullChecks": false,
        "target": "ES2017"
    }
}